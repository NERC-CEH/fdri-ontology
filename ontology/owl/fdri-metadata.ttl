#################################################################
#    Classes
#################################################################

###  http://fdri.ceh.ac.uk/vocab/metadata/ActivityType
:ActivityType rdf:type owl:Class ;
              rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                owl:allValuesFrom :ActivityTypeScheme
                              ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/ActivityTypeScheme
:ActivityTypeScheme rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#ConceptScheme> .


###  http://fdri.ceh.ac.uk/vocab/metadata/Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf <http://www.w3.org/ns/dcat#Resource> ,
                       <http://www.w3.org/ns/prov#Agent> .


###  http://fdri.ceh.ac.uk/vocab/metadata/Aggregation
:Aggregation rdf:type owl:Class ;
             rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :valueStatistic ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :periodicity ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :resolution ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                             ] ;
             rdfs:comment "Represents the aggregation of all values collected with a given frequency (fdri:resolution) over a given period of time (fdri:periodicity) using a given aggregation method (fdri:valueStatistic)."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/Annotation
:Annotation rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasValue ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasValueSeries ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :property ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/Catchment
:Catchment rdf:type owl:Class ;
           rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                             owl:allValuesFrom :CatchmentScheme
                           ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/CatchmentScheme
:CatchmentScheme rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#ConceptScheme> .


###  http://fdri.ceh.ac.uk/vocab/metadata/ConfigurationArgument
:ConfigurationArgument rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasValue ;
