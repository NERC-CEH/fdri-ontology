<http://fdri.ceh.ac.uk/vocab/metadata> rdf:type owl:Ontology ;
                                        owl:imports <http://data.ordnancesurvey.co.uk/ontology/spatialrelations/> ,
                                                    <http://www.opengis.net/ont/geosparql/1.1> ,
                                                    <http://www.w3.org/2006/time#2016> ,
                                                    <http://www.w3.org/ns/dcat3> ,
                                                    ssn:ext ,
                                                    <https://w3id.org/iadopt/ont/1.0.3> ;
                                        rdfs:comment "An onotology for the recording of dataset metadata, provenance information and related reference data for use in the fine-grained metadata store element of the FDRI project."@en ;
                                        rdfs:label "FDRI Fine-grained Metadata Store Ontology"@en ;
                                        owl:versionInfo "0.3 DRAFT (In Progress)" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2003/01/geo/wgs84_pos#alt
<http://www.w3.org/2003/01/geo/wgs84_pos#alt> rdf:type owl:AnnotationProperty ;
                                              rdfs:comment """The WGS84 altitude of a SpatialThing (decimal meters 
above the local reference ellipsoid).""" ;
                                              rdfs:label "altitude" ;
                                              rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
<http://www.w3.org/2003/01/geo/wgs84_pos#location> rdf:type owl:AnnotationProperty ;
                                                   rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/based_near> ;
                                                   rdfs:range <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  http://xmlns.com/foaf/0.1/based_near
<http://xmlns.com/foaf/0.1/based_near> rdf:type owl:AnnotationProperty .


